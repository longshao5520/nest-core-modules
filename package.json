{
  "name": "@longshao5520/nest-core-modules",
  "version": "0.1.6",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli/gen-config-types.ts --dts --format cjs,esm --sourcemap",
    "prepack": "pnpm run build",
    "clean": "rimraf dist"
  },
  "bin": {
    "longqi-gen-config-types": "dist/cli/gen-config-types.js"
  },
  "peerDependencies": {
    "@nestjs/common": ">=9",
    "@nestjs/core": ">=9",
    "@nestjs/swagger": ">=9",
    "@typegoose/typegoose": ">=9",
    "mongoose": ">=6 <9"
  },
  "devDependencies": {
    "@swc/core": "^1.15.3",
    "@types/node": "^24.10.1",
    "tsup": "^7.2.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "axios": "^1.13.2",
    "chokidar": "^4.0.3",
    "rxjs": "^7.8.2"
  }
}